    <script>
        $(document).ready(function () {

            $("#switch").on("click" , function(e){
                if (navigator.vibrate) {
                    navigator.vibrate(50);
                }
                if (e.target.checked) {
                    $("body").addClass("lava");
                } else {
                    $("body").removeClass("lava");
                }
            });
        });
    </script>

